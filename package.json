{
  "name": "advent-of-code",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "node -r ts-node/register --env-file=.env src/index.ts",
    "lint": "eslint --ext .js,.ts --quiet .",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "typecheck": "tsc --incremental false --noEmit",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gustavkj/advent-of-code.git"
  },
  "author": "Gustav Utterheim <gustav@utterheim.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gustavkj/advent-of-code/issues"
  },
  "engines": {
    "node": "^20.0.0",
    "npm": "^10.1.0"
  },
  "homepage": "https://github.com/gustavkj/advent-of-code#readme",
  "dependencies": {
    "aoc-loader": "1.4.4",
    "readline-sync": "1.4.10"
  },
  "devDependencies": {
    "@types/jest": "29.5.11",
    "@types/node": "20.8.9",
    "@types/readline-sync": "1.4.6",
    "@typescript-eslint/eslint-plugin": "6.9.0",
    "@typescript-eslint/parser": "6.9.0",
    "eslint": "8.52.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "17.1.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jest": "27.6.0",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "lint-staged": "15.0.2",
    "prettier": "3.1.1",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "typescript": "5.2.2"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml}": "prettier --write"
  }
}
